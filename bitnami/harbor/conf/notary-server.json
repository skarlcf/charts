{
  "server": {
    "http_addr": ":4443"
  },
  "trust_service": {
    "type": "remote",
    "port": "7899",
    "key_algorithm": "ecdsa"
  },
  "logging": {
    "level": "{{ .Values.logLevel }}"
  },
  "storage": {
    "backend": "postgres"
  },
  "auth": {
      "type": "token",
      "options": {
          "issuer": "harbor-token-issuer",
          "rootcertbundle": "/root.crt"
      }
  }
}
